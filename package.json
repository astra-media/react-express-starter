{
  "name": "react-express-starter",
  "version": "1.0.0",
  "description": "React express started kit",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm run dev --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm install && npm install --prefix frontend && npm run build --prefix frontend",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/astra-media/react-express-starter.git"
  },
  "keywords": [
    "react",
    "express",
    "starter",
    "kit"
  ],
  "author": "M. Sakeeb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/astra-media/react-express-starter/issues"
  },
  "homepage": "https://github.com/astra-media/react-express-starter#readme",
  "dependencies": {
    "express": "^4.21.2"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "dotenv": "^16.4.7",
    "nodemon": "^3.1.9"
  }
}
